CREATE TABLE tabPedidoTemp(
codigoPedido VARCHAR(10) PRIMARY KEY ,
dataPedido DATE,
UPC INT,
nome VARCHAR(50),
qtd INT,
valor INT,
frete INT,
email VARCHAR(50),
codigoComprador INT,
nomeComprador VARCHAR(50),
endereco VARCHAR(100),
cep INT,
UF VARCHAR(2),
pais VARCHAR(20)
);

\copy usuarios(
CodigoPedido,dataPedido,
UPC,
nome,
qtd ,
valor,
frete,
email,
codigoComprador,
nomeComprador,
endereco,
cep ,
UF ,
pais)
FROM 'D:/Juan/Downloads/pedidos.csv'
DELIMITER ','
CSV HEADER;
